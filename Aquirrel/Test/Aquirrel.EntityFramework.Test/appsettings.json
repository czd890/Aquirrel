      "LogLevel": {
        "Aquirrel": "Trace",
        "Default": "Trace"
      }
    },
    //控制台logger日志级别
    "Console": {
      "Default": "Debug"
    },
    "Config": {
      "Aquirrel.Logger.File.Test": {
        "Path": "log/logtest",
        "FileName": "root_[yyyyMMddHH]"
      },
      "Aquirrel.Logger.File.Test.TestService2": {
        "FileName": "Service2_[yyyyMMddHH]",
